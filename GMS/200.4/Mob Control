define(action,%arg1) // change action here

[ENABLE]
alloc(MobControl,128)

MobControl:
mov [edi+000004E0],action
mov eax,[edi+000004E0]
jmp 02850B84+6

02850B84:
jmp MobControl
db 90

[DISABLE]
02850B84: // CVecCtrlMob::WorkUpdateActive: E9 ? ? ? ? 8B ? ? ? 00 00 83 ? ? 0F ? ? ? ? ? FF [Address below]
mov eax,[edi+000004E0]
 
dealloc(MobControl)
