[enable]
aobscan(bullet_check,0F 84 ? ? 00 00 8D ? ? E8 ? ? ? ? 84 C0 0F 84 ? ? 00 00)
label(_bullet_check)
registersymbol(_bullet_check)

bullet_check:
_bullet_check: //Infinite Bullets
db 0F 85

[disable]
bullet_check:
_bullet_check: //0F 84 ? ? 00 00 8D ? ? E8 ? ? ? ? 84 C0 0F 84 ? ? 00 00
db 0F 84
unregistersymbol(_bullet_check)